<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.facility.model.facility">
<class name="FacilityDetailsImp" table="facility_details">
		<id name="Id" column="id"/>
	
		<property name="fdetail_id" type="string" column="FACILITY_DETAILS_ID"/>
	
		<property name="description" type="string" column="	DESCRIPTION"/>
		<property name="note" type="string" column="NOTE"/>	
	</class>
</hibernate-mapping>